/* 搜索 */
.search{
  width: 100%;
  height:40px;
   display:flex;
   justify-content: center;
   align-items: center;
   position:relative;    
   top:4px;  
}
.search-in{
  width:90%;
  height:40px;
  display: flex;
  align-items: center;  
  background-color:#e9e9e9; 
  border-radius: 2%;
  
}
  .search-in image {
    height:30px;
    width:30px;
    position:relative;    
    left:4px;
  }
  .search-in text {
    position:relative;    
    left:4px;
  }
  .list-all{
  display:flex;
  align-items:center;
  justify-content:center;
  height:120px;
  width:100%;

}
.list-second{
  height:100px;
  width:90%;
  display:flex;
  
}
.list-second image{
  height:100px;
  width:35%;
  
}
.list-right{
  height:100px;
  width:65%;
}

.title-all{
  display:flex;
  justify-content:space-between;

}
.title-logn{
  position:relative;
  top:2px;    
  font-size:30rpx;
  height:30px;
  width:40px;
  color:white;
  background-color: #00C777;

}
.title{
  position:relative;  
  left:10px;
  font-size:30rpx;
  font-weight:600;
}
/* 收藏 */
.collect image{
  height:20px;
  width:20px;

}
.list-right-text{
  font-size:20rpx;
  position:relative;
  top:7px;    
  left:10px;
  justify-content:space-between;

}
.list-right-in{
  position:relative;
  top:14px;    
  left:10px;
  width:95%;
  height: 10px;
  display:flex;
  align-items:center;
  justify-content:space-between;
  color:#FF0000;

}
.list-right-last{
  position:relative;
  top:26px;    
  left:10px;
  height:20px;
  width:32px;
  border:1px;
  border-style: solid;
}
/* 分割线 */
.divLine{
 position:relative;
 top:1px;    
 background: black;
 width: 100%;
 height: 1rpx;
}
/* 代理 */
.agent-all{
  height:20px;
  width: 90%;
  display:flex;
  position:relative;
  left:20px;   

}
.agent{
  height:20px;
  width: 70%;
  background-color: #FFFBE5;
  border-radius: 5%;
  display:flex;
  align-items:center;

}
.agent image{
  position:relative;
  left:4px; 
}
.agent text{
  position:relative;
  left:5px; 
}
/* 没有记录 */
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.container image{
  height:150px;
  width:150px;
}
.container text{
  font-size:15px;
  color:#979797;
}