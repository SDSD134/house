/* 搜索 */
.search{
  width: 100%;
  height:40px;
   display:flex;
   justify-content: center;
   align-items: center;
   position:relative;    
   top:10px;  
}
.search-in{
  width:90%;
  height:40px;
  display: flex;
  align-items: center;  
  background-color:#e9e9e9; 
  border-radius: 2%;
  
}
  .search-in image {
    height:30px;
    width:30px;
    position:relative;    
    left:4px;
  }
  .input {
    position:relative;    
    left:4px;
  }
  /* 没有记录 */
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.container image{
  height:150px;
  width:150px;
}
.container text{
  font-size:15px;
  color:#979797;
}